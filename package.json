{
  "name": "sgu",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 3000",
    "build": "npm run search:index && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro && stylelint '**/*.css'",
    "format": "prettier --write .",
    "check": "astro check",
    "search:index": "tsx scripts/generate-search-index.ts",
    "deploy": "npm run build && npx wrangler pages deploy dist",
    "deploy:production": "npm run build && npx wrangler pages deploy dist --branch=main",
    "cf:login": "npx wrangler login",
    "cf:deploy": "npx wrangler pages deploy dist --project-name=usebastian-portfolio"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/mdx": "^4.3.11",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/rss": "^4.0.13",
    "@astrojs/sitemap": "^3.6.0",
    "@iconify-json/carbon": "^1.2.15",
    "@iconify-json/lucide": "^1.2.74",
    "@iconify-json/simple-icons": "^1.2.59",
    "@iconify-json/tabler": "^1.2.23",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "alpinejs": "^3.15.2",
    "astro": "^5.15.9",
    "astro-compress": "^2.3.8",
    "astro-font": "^1.1.0",
    "astro-icon": "^1.1.5",
    "astro-iconify": "^1.2.0",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "fuse.js": "^7.1.0",
    "mermaid": "^11.12.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "sharp": "^0.34.5",
    "shiki": "^3.15.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tsx": "^4.21.0"
  }
